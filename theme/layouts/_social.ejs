<ul class="share-buttons">
  <% site.shareButtons.forEach(({name, image, path, text}) => { %>
    <li>
      <a href="<%= path + 'https://fuckyeahbutton.org/' + (text ? text : '') %>" title="Share on <%= name %>" target="_blank">
        <img alt="Share on <%= name %>" src="<%= image %>" />
      </a>
    </li>
  <% }); %>
</ul>
